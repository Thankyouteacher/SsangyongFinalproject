<script type="text/javascript">
$().ready(function()
{
	
	$("#agree_all").click(function()
	{
		if($("#agree_all").prop("checked"))
		{
			$("#agree_1st").prop("checked", true);
			$("#agree_2nd").prop("checked", true);
		}
		
		if(!$("#agree_all").prop("checked"))
		{
			$("#agree_1st").prop("checked", false);
			$("#agree_2nd").prop("checked", false);
		}
		
	});	// 전체 동의 클릭 시 체크, 체크 해제 메소드
	
	$("#agree_1st").click(function()
	{
		if($("#agree_1st").prop("checked") && $("#agree_2nd").prop("checked"))
		{
			$("#agree_all").prop("checked", true)
		}
		
		if(!$("#agree_1st").prop("checked") || !$("#agree_2nd").prop("checked"))
		{
			$("#agree_all").prop("checked", false)
		}
	});	// 첫번째 동의를 풀면 전체 동의 해제
	
	$("#agree_2nd").click(function()
	{
		if($("#agree_1st").prop("checked") && $("#agree_2nd").prop("checked"))
		{
			$("#agree_all").prop("checked", true)
		}
		
		if(!$("#agree_1st").prop("checked") || !$("#agree_2nd").prop("checked"))
		{
			$("#agree_all").prop("checked", false)
		}
	}); // 두번째 동의를 풀면 전체 동의 해제
	
	$("#btn_agree").click(function()
	{
		if(!$("#agree_1st").prop("checked") || !$("#agree_2nd").prop("checked"))
		{
			alert("약관에 모두 동의해주세요.");
			$("#input_1st").focus();
			return;
		}
		
		$("#preForm").submit();
	}); // 동의합니다 버튼을 눌렀을 때 약관 동의했는지 체크
	
	

});

</script>

